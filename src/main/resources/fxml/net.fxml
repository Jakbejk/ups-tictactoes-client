<?xml version="1.0" encoding="UTF-8"?>

<?import cz.zcu.fav.kiv.ups.sp.client.ui.element.textfield.NumberTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx"
      alignment="CENTER"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="cz.zcu.fav.kiv.ups.sp.client.ui.controller.NetController"
      prefHeight="400.0" prefWidth="600.0">
    <Label fx:id="infoLabel">
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0"/>
        </VBox.margin>
    </Label>
    <Label text="Host name:"/>
    <TextField fx:id="hostNameTextField">
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0"/>
        </VBox.margin>
    </TextField>
    <Label text="Port:"/>
    <NumberTextField fx:id="portNumberTextField">
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0"/>
        </VBox.margin>
    </NumberTextField>
    <Button text="Submit" onAction="#connect">
        <VBox.margin>
            <Insets top="10.0" bottom="10.0" left="10.0" right="10.0"/>
        </VBox.margin>
    </Button>
</VBox>
